    <section class="s-extra">

        <div class="row top">

            <div class="left col-eight md-six tab-full popular">
                <h3>Popular Posts</h3>

                <div class="block-1-2 block-m-full popular__posts">
                    <% for(var i=0; i<blogs.length; i++) { %>
                        <article class="col-block popular__post">
                            <a href="#0" class="popular__thumb">
                                <img src="<%-blogs[i].title_photo_url%>" alt="">
                            </a>
                            <h5 class="bold left">
                                <a href="#0">
                                    <%-blogs[i].blog_title%>
                                </a>
                            </h5>
                            <p class="left small">
                                <a href="#0">
                                    <%-blogs[i].timestamp%>
                                </a>
                            </p>
                        </article>
                        <% } %>
                </div>
                <!-- end popular_posts -->
            </div>
            <!-- end popular -->

            <div class="left col-four md-six tab-full about">
