                    </li>
                    <li>
                        <a href="#0"><i class="fa fa-instagram" aria-hidden="true"></i></a>
                    </li>
                    <li>
                        <a href="#0"><i class="fa fa-pinterest" aria-hidden="true"></i></a>
                    </li>
                </ul>
                <!-- end header__social -->
            </div>
            <!-- end about -->

        </div>
        <!-- end row -->

        <div class="row bottom tags-wrap">
            <div class="col-full tags">
                <h3>Tags</h3>

                <div class="tagcloud">
                    <% for(var i=0; i<topics.length; i++) { %>
                        <a href="#0">
                            <%-topics[i].topic_name%>
                        </a>
                        <% } %>

                </div>
                <!-- end tagcloud -->
            </div>
            <!-- end tags -->
        </div>
